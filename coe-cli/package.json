{
  "name": "coe",
  "version": "0.0.1",
  "description": "Power Platform COE Toolkit Command Line Interface",
  "bin": {
    "coe": "./coe"
  },
  "main": "index.js",
  "scripts": {
    "start": "npm run dev",
    "test": "jest",
    "build": "tsc",
    "lint": "eslint . & echo 'lint complete'",
    "dev": "ts-node-dev --respawn --transpile-only ./src/index.js",
    "prod": "tsc && node ./built/src/index.js",
    "debug": "tsc && node --inspect ./built/src/index.js",
    "docs": "tsc && jsdoc -r src -d docs -c jsdoc.json"
  },
  "author": "PowerCAT",
  "license": "MIT",
  "dependencies": {
    "@azure/msal-node": "^1.1.0",
    "axios": "^0.21.1",
    "azure-devops-node-api": "^10.2.2",
    "colors": "^1.4.0",
    "commander": "^7.2.0",
    "inquirer": "^8.0.0",
    "querystring": "^0.2.1",
    "typescript": "^4.2.4",
    "url": "^0.11.0",
    "util": "^0.12.3"
  },
  "devDependencies": {
    "@types/jest": "^26.0.23",
    "@types/node": "^15.3.0",
    "@typescript-eslint/eslint-plugin": "^4.23.0",
    "@typescript-eslint/parser": "^4.23.0",
    "better-docs": "^2.3.2",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "jest": "^26.6.3",
    "jest-mock-extended": "^1.0.14",
    "jsdoc": "~3.6.7",
    "ts-jest": "^26.5.6",
    "ts-node": "^9.1.1",
    "ts-node-dev": "^1.1.6"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ]
  }
}
